                                            <td>Order Imported</td>
                                        </tr>
                                        <tr>
                                            <td>01/31/2015</td>
                                            <td><a href="#">922942</a></td>
                                            <td>Manual Hold</td>
                                        </tr>
                                </table>
                            </div>

                            <label>Task List</label>
                            <div class="table-responsive">
                                <table class="table table-hover table-striped">
                                    <thead>
                                        <tr>
                                            <th></th>
                                            <th>Date</th>
                                            <th>Note</th>
