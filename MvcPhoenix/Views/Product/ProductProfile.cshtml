                                @Html.CheckBoxFor(m => m.HSPhysCorrosiveOpt)
                                @Html.LabelFor(m => m.HSPhysCorrosiveOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSPhysFlammableOpt)
                                @Html.LabelFor(m => m.HSPhysFlammableOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSPhysOxidizerOpt)
                                @Html.LabelFor(m => m.HSPhysOxidizerOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSPhysReactiveOpt)
                                @Html.LabelFor(m => m.HSPhysReactiveOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSPhysToxicOpt)
                                @Html.LabelFor(m => m.HSPhysToxicOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSPhysPeroxideOpt)
                                @Html.LabelFor(m => m.HSPhysPeroxideOpt, new {@class = "control-label"})<br />
