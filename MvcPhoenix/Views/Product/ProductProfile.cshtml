                                <label>HMIS Ratings</label>
                                <table class="table table-bordered">
                                    <tbody>
                                        <tr>
                                            <th style="background-color: blue; color: white">@Html.LabelFor(m => m.HMISHealth, new {@class = "control-label"})</th>
                                            <th style="text-align: center">1</th>
                                        </tr>
                                        <tr>
                                            <th style="background-color: red; color: white">@Html.LabelFor(m => m.HMISFlammability, new {@class = "control-label"})</th>
