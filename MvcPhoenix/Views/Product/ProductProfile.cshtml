                                @Html.LabelFor(m => m.HSEquipGlovesOpt, new {@class = "control-label"})<br />
                                @Html.TextBoxFor(m => m.HSEquipGloveType, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSEquipBootiesOpt)
                                @Html.LabelFor(m => m.HSEquipBootiesOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSEquipCleanRoomOpt)
                                @Html.LabelFor(m => m.HSEquipCleanRoomOpt, new {@class = "control-label"})<br />
