                                @Html.LabelFor(m => m.HSHealthReprodToxinOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSHealthCorrosiveHzdOpt)
                                @Html.LabelFor(m => m.HSHealthCorrosiveHzdOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSHealthSensitizerOpt)
                                @Html.LabelFor(m => m.HSHealthSensitizerOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSHealthCarcinogenOpt)
                                @Html.LabelFor(m => m.HSHealthCarcinogenOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSHealthIrritantOpt)
                                @Html.LabelFor(m => m.HSHealthIrritantOpt, new {@class = "control-label"})<br />
                            </div>
                            <div class="form-group col-md-3">
