                                @Html.LabelFor(m => m.HSEquipArmSleevesOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSEquipHalfMaskOpt)
                                @Html.LabelFor(m => m.HSEquipHalfMaskOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSEquipFullMaskOpt)
                                @Html.LabelFor(m => m.HSEquipFullMaskOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSEquipFullSuitOpt)
                                @Html.LabelFor(m => m.HSEquipFullSuitOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.HSEquipOtherOpt)
                                @Html.LabelFor(m => m.HSEquipOtherOpt, new {@class = "control-label"})<br />
