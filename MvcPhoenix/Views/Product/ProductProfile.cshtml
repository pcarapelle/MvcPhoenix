@model MvcPhoenix.Models.ProductViewModels

@{
    ViewBag.Title = "Product Profile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- container-fluid -->
<div class="container-fluid">

    <!-- Page Heading -->
    <div class="row">
        <div>
            <img src="~/Content/images/logos/acme.svg" style="float: right" alt="acme logo">
        </div>
        <div class="col-lg-12">
            <h1 class="page-header">@ViewBag.Title <small>Sample Product Guide</small></h1>
        </div>
    </div>
    <!-- /.Page Heading -->
    <!-- Form -->
    <form role="form">
        <div class="row">
            <div class="col-lg-12">

                <section id="productsearch">
                    <div class="form-group col-md-12">
                        <input type="search" class="form-control" placeholder="Product Search...">
                    </div>
                </section>

                <section id="productidinfo">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title" data-toggle="collapse" data-target="#prodid"><i class="fa fa-file-text-o fa-fw"></i> Product Identification</h3>
                        </div>
                        <div id="prodid" class="panel-body">
                            <label id="discontinued" class="col-md-12 collapse form-control" style="background-color: red; color: white"> Discontinued Product</label>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(m => m.ClientName, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.ClientName, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-3">
                                @Html.LabelFor(m => m.ClientDivision, new { @class = "control-label" })
                                @Html.TextBoxFor(m => m.ClientDivision, new { @class = "form-control" })
                            </div>
                            <div class="form-group col-md-3">
                                @Html.LabelFor(m => m.ClientBusUnit, new { @class = "control-label" })
                                @Html.TextBoxFor(m => m.ClientBusUnit, new { @class = "form-control" })
                            </div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(m => m.ProductXRefList, new {@class = "control-label"})<br />
                                <textarea rows="5" class="form-control"></textarea>
                            </div>
                            <div class="form-group col-md-3">
                                @Html.LabelFor(m => m.ProfileRevDate, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.ProfileRevDate, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-3">
                                @Html.LabelFor(m => m.MSDSRevDate, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.MSDSRevDate, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-3">
                                @Html.LabelFor(m => m.LabelRevDate, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.LabelRevDate, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-3">
                                @Html.CheckBoxFor(m => m.GlobalProductOpt)
                                @Html.LabelFor(m => m.GlobalProductOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.AccuracyVerifiedOpt)
                                @Html.LabelFor(m => m.AccuracyVerifiedOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.DiscontinuedOpt)
                                @Html.LabelFor(m => m.DiscontinuedOpt, new {@class = "control-label"})
                            </div>
                            <div class="clearfix"></div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(m => m.ProductCode, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.ProductCode, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(m => m.ProductName, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.ProductName, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-2">
                                @Html.LabelFor(m => m.ProductEquivalence, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.ProductEquivalence, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-2">
                                @Html.LabelFor(m => m.ProductShelfLife, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.ProductShelfLife, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-2">
                                @Html.LabelFor(m => m.ProductHazardClass, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.ProductHazardClass, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-2">
                                @Html.LabelFor(m => m.ProductSG, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.ProductSG, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-2">
                                @Html.LabelFor(m => m.ProductDensity, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.ProductDensity, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-2">
                                @Html.LabelFor(m => m.ProductPHVal, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.ProductPHVal, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-8">
                                @Html.LabelFor(m => m.PackagePartNumber, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.PackagePartNumber, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-4">
                                @Html.LabelFor(m => m.ProductRestrictedQty, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.ProductRestrictedQty, new {@class = "form-control"})
                            </div>
                            <div class="table-responsive col-md-12">
                                <table class="table table-hover table-striped">
                                    <thead>
                                        <tr>
                                            <th></th>
                                            <th>Part Number</th>
                                            <th>Size</th>
                                            <th>Weight</th>
                                            <th>Package</th>
                                            <th>DOT Spec</th>
                                            <th>UN Spec</th>
                                            <th>Torque</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td><i class="fa fa-minus-circle fa-fw" style="color: red"></i></td>
                                            <td>CMC-84</td>
                                            <td>QT</td>
                                            <td></td>
                                            <td>Plastic, Round, 38MM, Black Teflon Cap</td>
                                            <td>2E</td>
                                            <td>1H2/Y23/S</td>
                                            <td></td>
                                        </tr>
                                        <tr>
                                            <td><i class="fa fa-minus-circle fa-fw" style="color: red"></i></td>
                                            <td>CMC-84</td>
                                            <td>GAL</td>
                                            <td></td>
                                            <td>Plastic, Round, 38MM, Black Teflon Cap</td>
                                            <td>2E</td>
                                            <td>1H2/Y23/S</td>
                                            <td></td>
                                        </tr>
                                        <tr>
                                            <td><i class="fa fa-minus-circle fa-fw" style="color: red"></i></td>
                                            <td>CMC-84</td>
                                            <td>5GAL</td>
                                            <td></td>
                                            <td>Plastic, Round, 38MM, Black Teflon Cap</td>
                                            <td>2E</td>
                                            <td>1H2/Y23/S</td>
                                            <td></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </section>

                <section id="specialhandlinginfo">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title" data-toggle="collapse" data-target="#special"><i class="fa fa-file-text-o fa-fw"></i> Special Handling & Storage</h3>
                        </div>
                        <div id="special" class="panel-body collapse">
                            <div class="form-group col-md-3">
                                @Html.CheckBoxFor(m => m.SHCleanRoomGMPOpt)
                                @Html.LabelFor(m => m.SHCleanRoomGMPOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.SHNitrogenBlanketOpt)
                                @Html.LabelFor(m => m.SHNitrogenBlanketOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.SHMoistureSensitiveOpt)
                                @Html.LabelFor(m => m.SHMoistureSensitiveOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.SHMixWellOpt)
                                @Html.LabelFor(m => m.SHMixWellOpt, new {@class = "control-label"})<br />
                            </div>
                            <div class="form-group col-md-3">
                                @Html.CheckBoxFor(m => m.SHNoPackOverRhOpt)
                                @Html.LabelFor(m => m.SHNoPackOverRhOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.SHFlammableGroundOpt)
                                @Html.LabelFor(m => m.SHFlammableGroundOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.SHTorqueOpt)
                                @Html.LabelFor(m => m.SHTorqueOpt, new {@class = "control-label"})<br />
                                @Html.LabelFor(m => m.SHTorqueInp, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.SHTorqueInp)
                            </div>
                            <div class="form-group col-md-3">
                                @Html.LabelFor(m => m.SHFlashPointInp, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.SHFlashPointInp)<br />
                                @Html.CheckBoxFor(m => m.SHSpecialBlendOpt)
                                @Html.LabelFor(m => m.SHSpecialBlendOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.SHHeatFillingOpt)
                                @Html.LabelFor(m => m.SHHeatFillingOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.SHFreezerStorageOpt)
                                @Html.LabelFor(m => m.SHFreezerStorageOpt, new {@class = "control-label"})<br />
                            </div>
                            <div class="form-group col-md-3">
                                @Html.CheckBoxFor(m => m.SHRefridgeStorageOpt)
                                @Html.LabelFor(m => m.SHRefridgeStorageOpt, new {@class = "control-label"})<br />
                                @Html.CheckBoxFor(m => m.SHFlammableStorageOpt)
                                @Html.LabelFor(m => m.SHFlammableStorageOpt, new {@class = "control-label"})<br />
                                <b>Req By:</b>
                                @Html.CheckBoxFor(m => m.SHReqByClientOpt)
                                @Html.LabelFor(m => m.SHReqByClientOpt, new {@class = "control-label"})
                                @Html.CheckBoxFor(m => m.SHReqByCMCOpt)
                                @Html.LabelFor(m => m.SHReqByCMCOpt, new {@class = "control-label"})
                                <br />
                                @Html.LabelFor(m => m.SHOtherStorage, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.SHOtherStorage)
                            </div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(m => m.SHHeatingInstructions, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.SHHeatingInstructions, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(m => m.SHMixingInstructions, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.SHMixingInstructions, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(m => m.SHOtherInstructions, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.SHOtherInstructions, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(m => m.SHShippingSOP, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.SHShippingSOP, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(m => m.SHNormalAppearance, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.SHNormalAppearance, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-6">
                                @Html.LabelFor(m => m.SHRejectionCriterion, new {@class = "control-label"})
                                @Html.TextBoxFor(m => m.SHRejectionCriterion, new {@class = "form-control"})
                            </div>
                            <div class="form-group col-md-3">
                                <table class="table table-bordered">
                                    <tbody>
                                        <tr>
                                            <th style="background-color: yellow">@Html.LabelFor(m => m.SHRightToKnowOpt, new {@class = "control-label"})</th>
                                            <th style="text-align: center">@Html.CheckBoxFor(m => m.SHRightToKnowOpt)</th>
                                        </tr>
                                        <tr>
                                            <th style="background-color: yellow">@Html.LabelFor(m => m.SHFireListOpt, new {@class = "control-label"})</th>
                                            <th style="text-align: center">@Html.CheckBoxFor(m => m.SHFireListOpt)</th>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <div class="form-group col-md-3">
                                <table class="table table-bordered">
                                    <tbody>
                                        <tr>
                                            <th style="background-color: yellow">@Html.LabelFor(m => m.SHFreezeListOpt, new {@class = "control-label"})</th>
                                            <th style="text-align: center">@Html.CheckBoxFor(m => m.SHFreezeListOpt)</th>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <div class="form-group col-md-3">
                                <table class="table table-bordered">
                                    <tbody>
                                        <tr>
                                            <th style="background-color: yellow">@Html.LabelFor(m => m.SHSARAOpt, new {@class = "control-label"})</th>
                                            <th style="text-align: center">@Html.CheckBoxFor(m => m.SHSARAOpt)</th>
                                        </tr>
                                        <tr>
                                            <th style="background-color: green; color: white">@Html.LabelFor(m => m.SHSARA313Opt, new {@class = "control-label"})</th>
                                            <th style="text-align: center">@Html.CheckBoxFor(m => m.SHSARA313Opt)</th>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <div class="form-group col-md-3">
                                <table class="table table-bordered">
                                    <tbody>
                                        <tr>
                                            <th style="background-color: purple; color: white">@Html.LabelFor(m => m.SHSARA302Opt, new {@class = "control-label"})</th>
                                            <th style="text-align: center">@Html.CheckBoxFor(m => m.SHSARA302Opt)</th>
                                        </tr>
                                        <tr>
                                            <th style="background-color: red; color: white">@Html.LabelFor(m => m.SHEPABiocideOpt, new {@class = "control-label"})</th>
                                            <th style="text-align: center">@Html.CheckBoxFor(m => m.SHEPABiocideOpt)</th>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </section>

