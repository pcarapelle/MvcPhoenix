                                @Html.LabelFor(m => m.TOEEyeHazardOpt, new {@class = "control-label"})<br />
                            </div>
                        </div>
                    </div>
                </section>

                <section id="ghsinfo">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title" data-toggle="collapse" data-target="#ghs"><i class="fa fa-file-text-o fa-fw"></i> GHS Information</h3>
                        </div>
                        <div id="ghs" class="panel-body collapse">
                            <div class="form-group col-md-3">
                                <label>P & H Statements</label>
                                <input class="form-control" placeholder="Search for statements"><br />
                                <button type="button" class="btn btn-info btn-block">Update P&H Data</button>
                            </div>
                            <div class="form-group table-responsive col-md-9">
                                <table class="table table-striped">
                                    <tbody>
                                        <tr>
